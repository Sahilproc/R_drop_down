Dim r As Integer, Brand As String
Set ws = ActiveSheet
ws.Range("A1:L1").AutoFilter
r = 1
Do
  r = r + 1
  Brand = ws.Range("L" & r).Value
  On Error Resume Next
  If Sheets(Brand) Is Nothing Then
    ws.Range("A1:L1").AutoFilter Field:=1, Criterial:=Brand
    ws.Range("A1").CurrentRegion.SpecialCells(xlCellTypeVisible).Copy
    Sheets.Add.Name = Brand
    Sheets(Brand).Paste
    ws.ShowAllData
  End If
Loop While ws.Range("A" & r + 1).Value <> ""
End Sub
